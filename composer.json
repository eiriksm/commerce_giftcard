{
    "name": "drupal/commerce_giftcard",
    "type": "drupal-module",
    "description": "Allows to use and buy gift cards.",
    "homepage": "http://drupal.org/project/commerce_giftcard",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "drupal": {
            "type": "composer",
            "url":  "https://packages.drupal.org/8"
        }
    },
    "require": {
        "drupal/core": "^8.8 || ^9",
        "drupal/commerce": "^2"
    },
    "require-dev": {
        "mglaman/phpstan-drupal": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^7"
    }
}
